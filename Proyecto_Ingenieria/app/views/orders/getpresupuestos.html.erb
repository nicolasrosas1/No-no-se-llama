<h1>Presupuestos</h1>
<table class="table table-sm table-dark">
<tr>
  <th style = "background-color:#87A0F5">Orden</th>
  <th style = "background-color: #87A0F5">Material</th>
  <th style = "background-color: #87A0F5">Cantidad</th>
  <th style = "background-color: #87A0F5"> Costo</th>
</tr>
<% @data2.each do |m| %>
<tr>
  <th><%= m['id_orden'] %></th>
  <th><%= Material.find_by(id: MaterialsOrder.find_by(order_id: m["id_orden"]).material_id).nombre %></th>
  <th><%= MaterialsOrder.find_by(order_id: m["id_orden"]).cant%></th>
  <th><%= m["precio"]%></th>

</tr>
<% end %>

<% if @prev_view == "new_material" %>
  <%= link_to "Volver", "/new_material"%>
<% end %>
